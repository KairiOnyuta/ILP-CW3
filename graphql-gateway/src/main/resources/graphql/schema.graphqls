type Drone {
  name: String
  id: Int
  cooling: Boolean
  heating: Boolean
  capacity: Float
  maxMoves: Int
  costPerMove: Float
  costInitial: Float
  costFinal: Float
}

input QueryFilterInput {
  attribute: String!
  operator: String!
  value: String!
}

type Query {
  dronesWithCooling(state: Boolean!): [Drone!]!
  droneDetails(id: Int!): Drone
  queryDrones(filters: [QueryFilterInput!]!): [Drone!]!
}
